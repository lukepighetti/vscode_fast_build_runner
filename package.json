{
  "name": "build-runner",
  "publisher": "LukePighetti",
  "displayName": "build_runner",
  "repository": "https://github.com/lukepighetti/vscode_fast_build_runner",
  "description": "Run build-runner easily with a keyboard shortcut",
  "icon": "docs/icon.png",
  "version": "0.7.0",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:dart",
    "workspaceContains:**/*.dart"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "build-runner.quickly",
        "title": "build quickly on this file",
        "category": "build_runner"
      }
    ],
    "keybindings": [
      {
        "command": "build-runner.quickly",
        "key": "ctrl+alt+b",
        "mac": "cmd+alt+b"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "vscode-test": "^1.5.0"
  }
}
